	<input  class="input-xl" id="searchString" name="searchString" placeholder="Que cherchez vous aujourd'hui?"/>
	<button class="btn btn-info" id="submitSearch" type="submit">Rechercher</button>

</div>
<div class="searchResultZone" >

<table class="table table-striped" id="searchTable"  > 
    <thead>
        <tr>
            
            <td></td>
            <td>Numéro d'inventaire</td>

            <td>Désignation</td>
            
            <td>Auteur(s)</td>
            
            <td>Matériaux</td>
            
            <td>Technique(s)</td>
            
            <td>Dernière modification</td>
           
        </tr>
    </thead>
    <tbody id="resultTableBody">

        <% oeuvres.forEach(function(o){ %>
            <tr class="darkenOnHover" onclick="location.href='/oeuvre/show/<%=o.id%>'">
                <td>
                <% if(o.verrou){ %>
                    <span class="glyphicon glyphicon-lock"></span>
                <% } else { %>

                <% } %>
                </td>
                <td><%- o.oeuvreId %></td>
                <td><%- o.designation %></td>
                <td><%- o.auteur %></td>
                <td><%- o.materiaux %></td>
                <td><%- o.techniques %></td>
                <td><%- o.updatedAt %></td>
            </tr>
       <% }); %>
    </tbody>
</table>
</div>